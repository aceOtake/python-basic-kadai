        "\n",
        "# 消費税を加えた計算結果を返す関数\n",
        "def calculate_price_with_tax(price:int ,tax:float)-> int:\n",
        "  # work_price = price / tax\n",
        "  work_price = price * (tax/100)\n",
        "  total_price = price + work_price\n",
        "  return total_price\n",
        "\n",
        "# 第1引数に商品の金額「110」、第2引数に消費税（10%）の「10」を設定する\n",
        "print(calculate_price_with_tax(110,10))\n",
        "\n"
      ]
